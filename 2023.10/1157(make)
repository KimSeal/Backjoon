#include <bits/stdc++.h>
using namespace std;
bool arr[5001];
int a, b;

int add_1(int x) {
    int y = 0;
    while (1) {
        if (arr[x % a] == true) {
            x++;
        }
        else {
            y++;
            if (y == b) {
                arr[x % a] = true;
                return x % a;
            }
            x++;
        }
    }
}
int main() {
    
    cin >> a >> b;
    int start = 0;
    cout << "<";
    for (int i = 0; i < a;i++) {
        start = add_1(start);
        cout << ++start;
        if (i != a - 1)cout << ", ";
    }
    cout << ">";
}
