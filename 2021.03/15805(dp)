#include <iostream>
using namespace std;
//zandi test
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	int leng;
	cin >> leng;
	int input1= (-1);
	int input2;
	int big=0;
	int result[200000];
	for (int i = 0; i < leng; i++) {
		result[i] = -2;
	}
	for (int i = 0; i < leng;i++) {
		cin >> input2;
		if (big < input2) {
			big = input2;
		}
		if (result[input2] == -2) {
			result[input2] = input1;
		}
		input1 = input2;
	}
	cout << big + 1 << endl;
	for (int i = 0; i < big+1 ;i++) {
		cout << result[i] << ' ';
	}
	return 0;
}
