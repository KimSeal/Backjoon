#include <bits/stdc++.h>
using namespace std;

int vec_1[500001];
int vec_2[500001];
int N, M;
int binarySearch(int a){
    int right = N-1;
    int left = 0;
    int mid;
    while(left <= right){
        mid = (left+right)/2;
        if(vec_1[mid]== a){
            return 1;
        }
        else if(vec_1[mid] > a){
            right = mid-1;
        }
        else if(vec_1[mid] < a){
            left = mid +1;
        }
    }
    return 0;
}
int main(){
    cin >> N;
    for(int i=0;i<N;i++){
        cin >> vec_1[i];
    }
    sort(vec_1, vec_1 + N);
    cin >> M;
    for(int i=0;i<M;i++){
        cin >> vec_2[i];
    }
    for(int i=0;i<M;i++){
        cout << binarySearch(vec_2[i]) << " ";
    }   
}
