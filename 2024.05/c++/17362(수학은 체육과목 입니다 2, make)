#include<bits/stdc++.h>
using namespace std;
//짝수 홀수로 나눠서 탐색을 줄이는 것이 가능할 것 같다.
//else if의 난잡한 사용을 막을 것을 고려
int main(){
    long long N;
    cin >> N;
    if(N%8 == 1) cout << 1;
    else if(N%8==2 || N%8 == 0) cout << 2;
    else if(N%8==3 || N%8 == 7) cout << 3;
    else if(N%8==4 || N%8 == 6) cout << 4;
    else if(N%8==5) cout << 5;
}
