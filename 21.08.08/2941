#include<stdio.h>
int main(){
    char Input[100];
	int Num=0;
	scanf("%s",Input);
	for(int i=0;Input[i]!='\0';i++){
		if(Input[i]=='='||Input[i]=='-'){
		}
		else if(Input[i]=='j'&&Input[i-1]=='l'){
		}
		else if(Input[i]=='j'&&Input[i-1]=='n'){
		}
		else if(Input[i]=='z'&&Input[i-1]=='d'&&Input[i+1]=='='){
		}
		else{
			Num+=1;
		}
	}
	printf("%d",Num);
	return 0;
}
