#include<bits/stdc++.h>
using namespace std;
int main() {
    string str;
    int result = 0;
    char temp = '*';
    cin >> str;
    for (int i = 0; i < str.size();i++) {
        if (temp != str[i]) {
            result += 10;
            temp = str[i];
        }
        else {
            result += 5;
        }
    }
    cout << result;
}
