#include <bits/stdc++.h>
using namespace std;
int main(){
    int N, M;
    cin >> N >> M;
    if((N == 0 && M == 2) ||
       (N == 2 && M == 5) ||
       (N == 5 && M == 0) 
      ) cout << ">";
    else if((M == 0 && N == 2) ||
       (M == 2 && N == 5) ||
       (M == 5 && N == 0) 
      ) cout << "<";
    else if((N==M) && (N == 0 || N == 2 || N == 5)){
        cout << "=";
    }
    else if(N == 0 || N == 2 || N == 5){
        cout << ">";
    }
    else if(M == 0 || M == 2 || M == 5){
        cout << "<";
    }
    else cout << "=";
}
