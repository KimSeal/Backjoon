#include<bits/stdc++.h>
using namespace std;
int main() {
    int a, b, c, d, e, f, input0, result;
    result = 0;
    for (int i = 0; i < 3; i++) {
        result *= 60;
        cin >> input0;
        result += input0;
    }
    a = result;
    result = 0;
    for (int i = 0; i < 3; i++) {
        result *= 60;
        cin >> input0;
        result += input0;
    }
    b = result;
    result = 0;
    for (int i = 0; i < 3; i++) {
        result *= 60;
        cin >> input0;
        result += input0;
    }
    c = result;
    result = 0;
    for (int i = 0; i < 3; i++) {
        result *= 60;
        cin >> input0;
        result += input0;
    }
    d = result;
    result = 0;
    for (int i = 0; i < 3; i++) {
        result *= 60;
        cin >> input0;
        result += input0;
    }
    e = result;
    result = 0;
    for (int i = 0; i < 3; i++) {
        result *= 60;
        cin >> input0;
        result += input0;
    }
    f = result;
    b -= a;
    d -= c;
    f -= e;
    cout << b / 3600 << " " << (b % 3600) / 60 << " " << b % 60 << "\n";
    cout << d / 3600 << " " << (d % 3600) / 60 << " " << d % 60 << "\n";
    cout << f / 3600 << " " << (f % 3600) / 60 << " " << f % 60;
}
