#include<bits/stdc++.h>
using namespace std;


int main() {
	int N;
	cin >> N;
	string str;
	while (N--) {
		cin >> str;
		if (str.size() % 2 == 0) {
			for (int i = 0; i < str.size();i+=2) {
				cout << str[i];
			}
			cout << "\n";
			for (int i = 1; i < str.size(); i+=2) {
				cout << str[i];
			}
			cout << "\n";
		}
		else {
			for (int i = 0; i < str.size(); i+= 2) {
				cout << str[i];
			}
			for (int i = 1; i < str.size(); i+= 2) {
				cout << str[i];
			}
			cout << "\n";
			for (int i = 1; i < str.size(); i+= 2) {
				cout << str[i];
			}
			for (int i = 0; i < str.size(); i+= 2) {
				cout << str[i];
			}
			cout << "\n";
		}
	}

}
