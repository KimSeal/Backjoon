#include <bits/stdc++.h>
using namespace std;

bool chk[5];
int main(){
    string str0;
    cin >> str0;
    for(int i=0;i<str0.size();i++){
        if(str0[i] == 'M'){
            chk[0] = true;
        }
        if(str0[i] == 'O'){
            chk[1] = true;
        }
        if(str0[i] == 'B'){
            chk[2] = true;
        }
        if(str0[i] == 'I'){
            chk[3] = true;
        }
        if(str0[i] == 'S'){
            chk[4] = true;
        }
    }
    if(chk[0] && chk[1] && chk[2] && chk[3] && chk[4]){
        cout << "YES";
    }
    else cout <<"NO";
}
