#include<stdio.h>
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    char Input[21];
	int num1, num2;
	scanf("%d",&num1);
	for(int j=0;j<num1;j++){
		scanf("%d",&num2);
		scanf("%s",Input);
		for(int i=0;Input[i]!='\0';i++){
			for(int k=0;k<num2;k++){
				printf("%c",Input[i]);
			}
		}
		printf("\n");
	}
	return 0;
}
