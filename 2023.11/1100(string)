#include<bits/stdc++.h>
using namespace std;

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    string str_0;
    int answer =0;
    for(int i=0;i<8;i++){
        cin >> str_0;
        for(int j=0;j<8;j++){
            if( (i+j)%2 ==0 && str_0[j]=='F') answer++;
        }
    }    
    cout << answer;
}
