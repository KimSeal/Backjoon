#include <bits/stdc++.h>
using namespace std;
int N;
string str_1;
char temp;
int main() {
    cin >> N;
    while (N--) {
        cin >> str_1;
        for (int i = 0; i < str_1.size(); i++) {
            if (str_1.c_str()[i] >= 'A' && str_1.c_str()[i] <= 'Z') {
                temp = str_1.c_str()[i] - 'A' + 'a';
                cout << temp;
            }
            else cout << str_1[i];
        }
        cout << "\n";
    }
}
